{
  "name": "learn-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "vite": "vite",
    "dev": "npm-run-all --parallel tcm:watch vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "tcm": "tcm src --include '**/*.module.css'",
    "tcm:watch": "tcm src --include '**/*.module.css' --watch",
    "deploy:frontend": "npm run build && cd ../server && serverless s3sync"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.0.2",
    "@mui/x-date-pickers": "^8.3.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "i18next": "^25.2.0",
    "joi": "^17.13.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-google-recaptcha": "^3.1.0",
    "react-i18next": "^15.5.1",
    "react-icons": "^5.5.0",
    "react-router": "^7.5.1",
    "react-switch": "^7.1.0",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@tailwindcss/vite": "^4.1.4",
    "@types/node": "^22.14.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/react-google-recaptcha": "^2.1.9",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^4.1.4",
    "typed-css-modules": "^0.9.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vite-plugin-svgr": "^4.3.0"
  }
}
